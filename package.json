  "readmeFilename": "README.md",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf www",
    "start": "ng serve",
    "start:dev": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:prod": "ng serve  --configuration production",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:ci": "ng build --configuration ci",
    "build:prod": "ng build --configuration production && npm run i18n:build",
    "i18n:build": "node scripts/node/build-i18n.js",
    "android:sync": "npx jetifier && ionic capacitor sync android",
    "android:start": "ionic capacitor run android -l --external",
    "android:build": "npx jetifier && ionic capacitor build android --no-open",
    "android:build:dev": "npx jetifier && ionic capacitor build android --no-open --configuration development",
    "android:build:prod": "npx jetifier && ionic capacitor build android --no-open --configuration production --release && npm run android:i18n:build",
    "android:i18n:build": "node scripts/node/build-i18n.js android/app/src/main/assets/public/assets/i18n",
    "android:package": "cd android && ./gradlew assembleDebug --warning-mode all",
    "android:package:prod": "cd android && ./gradlew assembleRelease",
    "android:list": "npx native-run android --list",
    "resources": "cordova-res ios && cordova-res android && node scripts/node/resources.js",
    "test": "ng test",
    "lint": "node --max-old-space-size=6096 node_modules/@angular/cli/bin/ng lint",
    "lint:fix": "node --max-old-space-size=6096 node_modules/@angular/cli/bin/ng lint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,html}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{css,json,md,scss}": "prettier --write"
  },
