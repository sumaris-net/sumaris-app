
{
  "manifest_version": 2,
  "name": "sumaris-app",
  "version": "2.9.19.7",
  "version_name": "2.9.19.7",
  "short_name": "SUMARiS",
  "description": "SUMARiS app",
  "author": "EIS <contact@e-is.pro>",
  "homepage": "https://github.com/sumaris-net/sumaris-app",
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "SUMARiS",
    "default_icon": {
      "32": "icon/logo_32px.png",
      "60": "icon/logo_60px.png",
      "96": "icon/logo_96px.png",
      "128": "icon/logo_128px.png",
      "192": "icon/logo_192px.png"
    }
  },
  "icons": {
    "32": "icon/logo_32px.png",
    "60": "icon/logo_60px.png",
    "96": "icon/logo_96px.png",
    "128": "icon/logo_128px.png",
    "192": "icon/logo_192px.png"
  }
}

